<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.exam.AuthController"
            prefHeight="200.0"
            prefWidth="400.0">
    <children>
        <Label layoutX="152.0" layoutY="44.0" text="Авторизация" />
        <TextField fx:id="usernameField" layoutX="100.0" layoutY="84.0" promptText="Имя пользователя" />
        <PasswordField fx:id="passwordField" layoutX="100.0" layoutY="123.0" promptText="Пароль" />
        <Button fx:id="loginButton" layoutX="149.0" layoutY="166.0" mnemonicParsing="false" text="Войти" onAction="#login" />
    </children>
</AnchorPane>
